#!/usr/bin/env sh

ulimit -s `ulimit -H -s`
if [ -z "$K_OPTS" ];
  then export K_OPTS="-Xms64m -Xmx1024m -Xss32m -XX:+TieredCompilation"
fi
if "$(dirname "$0")/../lib/scripts/checkJava"; then
  java -Djava.awt.headless=true $K_OPTS -ea -jar "$(dirname "$0")/../lib/java/k3.jar" -ktest "$@"
else
  exit 1
fi
