  op getName : OpDeclSet Qid Type -> Qid .
  eq getName(OPDS' (op Q : nil -> Ty [AS metadata(Str) AS'] .) OPDS, Q, Ty)
   = getName(Q, metadataParse(Str)) .
  eq getName(OPDS, Q, 'Qid) = getLabelName(OPDS, qid(substr(string(Q),1,length(string(Q))))) .
