  op eMetaLatexPrint0 : Module MembAxSet -> QidList .

  eq eMetaLatexPrint0(M, (mb T : S [none] .) MAS)
    = ('\n 'mb eMetaLatexPrint(M, T) 
                   ': eMetaLatexPrint(S) '. 
       eMetaLatexPrint0(M, MAS)) [owise] .
