// Testing mutually recursive uses of this.method

class OddEven {
  var n;
  method OddEven(x) {
    n = x;
  }
  method even() {
    if (n == 0) {
      return 1;
    }
    else {
      n = n - 1;
      return this.odd();
    }
  }
  method odd() {
    if (n == 0) {
      return 0;
    }
    else {
      n = n - 1;
      return this.even();
    }
  }
}

class Main {
  method Main() {
    print((new OddEven(17)).odd(), "\n");
  }
}

// 1
