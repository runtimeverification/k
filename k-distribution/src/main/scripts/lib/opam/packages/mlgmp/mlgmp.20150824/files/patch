+
+value _mlgmp_fr_equal(value a, value b)
+{
+#ifdef USE_MPFR
+  CAMLparam2(a, b);
+  CAMLreturn(mpfr_equal_p(*mpfr_val(a), *mpfr_val(b))
+	     ? Val_true : Val_false);
+#else
+  unimplemented(equal);
+#endif
+}
+
+
+value _mlgmp_fr_greater(value a, value b)
+{
+#ifdef USE_MPFR
+  CAMLparam2(a, b);
+  CAMLreturn(mpfr_greater_p(*mpfr_val(a), *mpfr_val(b))
+	     ? Val_true : Val_false);
+#else
+  unimplemented(greater);
+#endif
+}
+
