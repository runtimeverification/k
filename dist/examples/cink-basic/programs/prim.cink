 #include <iostream>
    bool isPrim(int n) {
          int d = 2;
          while (d*d <= n) {
            if (n % d == 0) return false;
            d = d + 1;
          }
          return true;
        }
    int main() {
         int n, p = 2;
         cout << "n = ";
         cin >> n;
         while (n > 0) {
           if (isPrim(p)) n = n - 1;
           p = p + 1;
         }
         cout << p;
         return 0;
    }

