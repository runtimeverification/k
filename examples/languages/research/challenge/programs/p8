var a,n,i,j,t;
          n = 10;
          a = malloc(n);
          n = n+-1;
          i = -1;
          while (++i <= n) do {
            *(a+i) = i;
          }
          i = -1; 
          while (++i <= n) do {
            j = i;
            while (++j <= n) do
              if *(a+i) <= *(a+j) then { 
                 t = *(a+i); 
                 *(a+i) = *(a+j);
                 *(a+j) = t;
              } else {}
            print(*(a+i));
          }

