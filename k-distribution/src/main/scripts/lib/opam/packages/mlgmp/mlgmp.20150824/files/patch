+value _mlgmp_fr_is_inf(value a)
+{
+#ifdef USE_MPFR
+  CAMLparam1(a);
+  CAMLreturn(mpfr_inf_p(*mpfr_val(a)) ? Val_true : Val_false);
+#else
+  unimplemented(is_nan);
+#endif
+}
