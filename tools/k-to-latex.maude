   = addParams(getName(Q, metadataParse(Str)),arity(TyL)) .
  eq getLabelName(OPDS, Q) = Q [owise] .
   
  op addParams : Qid Nat -> Qid .
  eq addParams(Q, N) = if latexCmd(Q) then qidList2Qid('\ensuremath '`{ Q genQids(N, '`{ '\_ '`}) '`}) else Q fi .

